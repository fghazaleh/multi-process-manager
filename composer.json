{
    "name": "fghazaleh/multi-thread-manager",
    "type": "library",
    "description": "Multi-thread manager using Symfony process component",
    "license": "MIT",
    "homepage": "https://github.com/fghazaleh/multi-thread-manager",
    "keywords": [
        "symfony process",
        "multi thread",
        "multi process",
        "async process",
        "async",
        "non-blocking",
        "multihreading",
        "asynchronous",
        "parallel",
        "threads",
        "queue"
    ],
    "authors": [
        {
            "name": "Franco Ghazaleh",
            "email": "franco.ghazaleh@gmail.com",
            "homepage": "https://ghazaleh.co/",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/process": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FGhazaleh\\MultiThreadManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FGhazaleh\\MultiThreadManager\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "scripts": {
        "csfix": "php-cs-fixer fix",
        "coverage": "vendor/bin/phpunit --coverage-html ./coverage && open ./coverage/index.html"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
